{
  "name": "@amory/git",
  "private": true,
  "scripts": {
    "postinstall": "npx npm-run-all --parallel postinstall:*",
    "postinstall:gitignore": "f () { ln -s env/git/.gitignore ../.. || exit 0; }; f",
    "postinstall:packagejson": "npx -p @amory/merge-json -c 'merge-json ../../package.json .package.json'",
    "preinstall": "f () { rm ../../.gitignore || exit 0; }; f"
  },
  "version": "2019.3.15-0"
}
