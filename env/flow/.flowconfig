[declarations]
.*/.*\.test.js

[ignore]
.*/\..*
.*/node_modules/.*

[include]
src/**/*.js

[libs]

[lints]
all=warn
dynamic-export=off
unclear-type=off
untyped-import=off

[options]
emoji=true
esproposal.class_instance_fields=warn
esproposal.class_static_fields=warn
esproposal.decorators=warn
esproposal.export_star_as=warn
esproposal.nullish_coalescing=warn
esproposal.optional_chaining=warn
include_warnings=true

[untyped]
src/vendor/*.js

[version]
^0.95.1
